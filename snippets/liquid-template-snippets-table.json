{
    "HTML Table": {
        "prefix": "table",
        "body": [
            "<table class=\"usr-width-100\">",
            "\t<thead>",
            "\t\t<tr>",
            "\t\t\t<th class=\"usr-width-${2:50}\">${3:content}</th>",
            "\t\t\t<th class=\"usr-width-${4:50}\">${5:content}</th>",
            "\t\t</tr>",
            "\t</thead>",
            "\t<tbody>",
            "\t\t<tr>",
            "\t\t\t<td>${6:content}</td>",
            "\t\t\t<td>${6:content}</td>",
            "\t\t</tr>",
            "\t</tbody>",
            "</table>"
        ],
        "description": "Use html to create a table with more customization."
    },
