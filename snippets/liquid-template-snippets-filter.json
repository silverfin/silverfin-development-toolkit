    },
    "strip filter": {
        "prefix": "strip",
        "body": "| strip",
        "description": "Remove leading and trailing whitespaces"
    },
    "slice filter": {
        "prefix": "slice",
        "body": "| slice:${1:start}, ${2:end}",
        "description": [
